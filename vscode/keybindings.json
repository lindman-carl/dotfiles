// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "ctrl+alt+b",
    "command": "editor.emmet.action.balanceOut"
  },
  {
    "key": "ctrl+alt+v",
    "command": "editor.emmet.action.balanceIn"
  },
  {
    "key": "cmd+d",
    "command": "-codegenx.open_CodeGenX"
  },
  {
    "key": "cmd+l",
    "command": "expandLineSelection",
    "when": "textInputFocus, editorFocus"
  },
  {
    "key": "cmd+l",
    "command": "-expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+l cmd+c",
    "command": "-extension.liveServer.goOffline",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+l cmd+o",
    "command": "-extension.liveServer.goOnline",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "cmd+n",
    "command": "explorer.newFile"
  },
  {
    "key": "cmd+g cmd+c",
    "command": "github.copilot.toggleCopilot"
  },
  {
    "key": "shift+cmd+a",
    "command": "editor.action.transformToUppercase"
  },
  {
    "key": "cmd+i",
    "command": "-inlineChat.start",
    "when": "inlineChatHasProvider && !editorReadonly"
  },
  {
    "key": "cmd+i",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+i",
    "command": "-editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+v",
    "command": "workbench.action.splitEditor"
  },
  {
    "key": "ctrl+shift+alt+cmd+7",
    "command": "-workbench.action.splitEditor"
  },
  {
    "key": "shift+cmd+f",
    "command": "extension.react-refactor.extractToFunction",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+g",
    "command": "-editor.action.previousMatchFindAction",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+g",
    "command": "typescript.goToSourceDefinition"
  },
  {
    "key": "alt+cmd+f",
    "command": "editor.action.formatSelection.multiple"
  },
  {
    "key": "shift+cmd+j",
    "command": "terminal.focus"
  },
  {
    "key": "cmd+8",
    "command": "-workbench.action.focusEighthEditorGroup"
  },
  {
    "key": "cmd+9",
    "command": "-workbench.action.lastEditorInGroup"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.evenEditorWidths"
  },
  {
    "key": "cmd+8",
    "command": "workbench.action.decreaseViewSize"
  },
  {
    "key": "cmd+9",
    "command": "workbench.action.increaseViewSize"
  },
  // makes terminal panel size when in terminalFocus
  {
    "key": "cmd+0",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "terminalFocus"
  },
  {
    "key": "shift+cmd+b",
    "command": "-workbench.action.tasks.build",
    "when": "taskCommandsRegistered"
  },
  {
    "key": "shift+cmd+b",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "alt+cmd+b",
    "command": "-workbench.action.toggleAuxiliaryBar"
  }
]